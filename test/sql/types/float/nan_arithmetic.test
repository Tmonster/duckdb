# name: test/sql/types/float/nan_arithmetic.test
# description: Test arithmetic on NaN values
# group: [float]

foreach type FLOAT DOUBLE

foreach val nan

# any arithmetic on a nan will result in a nan
query I
SELECT '${val}'::${type} + 1
----
nan

query I
SELECT '${val}'::${type} - 1
----
nan

query I
SELECT '${val}'::${type} * 1
----
nan

query I
SELECT '${val}'::${type} / 1
----
nan

query I
SELECT '${val}'::${type} % 1
----
nan

# test negation
query I
SELECT -('${val}'::${type})
----
nan

endloop

endloop

query I
select -320938.4298::DECIMAL(18,4);
----
-320938.4298


query I
SELECT ('nan'::DOUBLE) + 1::HUGEINT
----
nan
